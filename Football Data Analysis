    "top_players = pd.read_sql(\"\"\"/* Tope 20 players of 2008 having higher overall_rating*/\n",
    "SELECT pa.id,\n",
    "       p.player_name,\n",
    "\t   pa.date,\n",
    "       pa.overall_rating,\n",
    "       pa.potential,\n",
    "       pa.preferred_foot,\n",
    "       pa.attacking_work_rate,\n",
    "       pa.defensive_work_rate,\n",
    "       pa.crossing,\n",
    "       pa.finishing,\n",
    "       pa.heading_accuracy,\n",
    "       pa.short_passing,\n",
    "       pa.volleys,\n",
    "       pa.dribbling,\n",
    "       pa.curve,\n",
    "       pa.free_kick_accuracy,\n",
    "       pa.long_passing,\n",
    "       pa.ball_control,\n",
    "       pa.acceleration\n",
    "  FROM Player_Attributes pa\n",
    "  JOIN Player p\n",
    "    ON pa.player_api_id = p.id\n",
    " WHERE pa.date BETWEEN '2008-01-01' AND '2008-12-31'\n",
    " ORDER BY overall_rating DESC\n",
    " LIMIT 20;\"\"\", conn)\n",
    "top_players"
   ]
