    "total_penalties = pd.read_sql(\"\"\"SELECT p.player_name player_name, \n",
    "                                        SUM(pa.penalties) total_penalites\n",
    "                                   FROM player_attributes pa\n",
    "                                   JOIN player p\n",
    "                                     ON p.id = pa.player_api_id\n",
    "                                  GROUP BY 1\n",
    "                                  ORDER BY 2 DESC\n",
    "                                  LIMIT 10;\"\"\", conn)\n",
    "total_penalties\n",
